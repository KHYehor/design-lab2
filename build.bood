go_binary {
  name: "bood",

  // Go package to build.
  pkg: "github.com/KHYehor/design-lab2/build/cmd/bood",
  testPkg: "github.com/yk2hk/design-lab2/build/cmd/bood",
  // Build inputs.
  srcs: ["**/*.go", "go.mod"],
  outTestFile: "test1.txt",
  srcsExclude: ["vendor/**/*", "examples/**/*"],

  // Flag to also perform dependencies vendoring.
  vendorFirst: false
}
